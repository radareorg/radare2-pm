R2PM_BEGIN

R2PM_GIT "https://github.com/newlog/r2com"
R2PM_DESC "COM object identification plugin for radare2"

R2PM_INSTALL() {
    mkdir -p "${R2PM_ETCD}" || exit 1
    echo '$r2com=#!pipe python '"'$(pwd)'"'/r2com.py' > "${R2PM_ETCD}/r2com" || exit 1
}

R2PM_UNINSTALL() {
    rm -f "${R2PM_ETCD}"/r2com
}

R2PM_END
