R2PM_BEGIN

R2PM_GIT "https://github.com/radareorg/radare2-regressions"
R2PM_DESC "[r2-tool] radare2-regressions tools (provides r2r and r2-v)"

R2PM_INSTALL() {
	${R2PM_SUDO} ${MAKE} symstall PREFIX="${R2PM_PREFIX}" || exit 1
}

R2PM_UNINSTALL() {
	${R2PM_SUDO} ${MAKE} uninstall PREFIX="${R2PM_PREFIX}"
}

R2PM_END
