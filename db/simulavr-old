R2PM_BEGIN

R2PM_GIT git://git.savannah.nongnu.org/simulavr.git simulavr
R2PM_DESC "[syspkg] simulavr - avr simulator with gdb server"

R2PM_INSTALL() {
	__=""
	git reset --hard
	echo "${__}" | base64 -D | gunzip | patch -p1
	glibtoolize --force
	autoheader
	aclocal --install
	autoconf
	automake --force-missing --add-missing
	./configure --prefix="${R2PM_PREFIX}" || exit 1
	make -j 4 || exit -1
	make install || exit 1
}

R2PM_UNINSTALL() {
	make uninstall || exit 1
}

R2PM_END
