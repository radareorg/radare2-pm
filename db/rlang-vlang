R2PM_BEGIN

R2PM_GIT "https://github.com/radareorg/radare2-rlang"
R2PM_DESC "[rlang] V RLang plugin #!v"

R2PM_INSTALL() {
	${MAKE} -C vlang clean || exit 1
	${MAKE} -C vlang || exit 1
	${MAKE} -C vlang install R2PM_PLUGDIR="${R2PM_PLUGDIR}" || exit 1
}

R2PM_UNINSTALL() {
	${MAKE} -C vlang uninstall
}

R2PM_END
