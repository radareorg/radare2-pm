R2PM_BEGIN

R2PM_GIT "https://github.com/danigargu/ida-syms2elf"
R2PM_DESC "Export recognized symbols to ELF symbol table"

R2PM_INSTALL() {
	mkdir -p "${R2PM_ETCD}"
	echo '$syms2elf=#!pipe python '"'`pwd`'"'/syms2elf.py' > "${R2PM_ETCD}/syms2elf"
}

R2PM_UNINSTALL() {
	rm -f "${R2PM_ETCD}"/syms2elf
}

R2PM_END
