R2PM_BEGIN

R2PM_GIT "https://github.com/nowsecure/r2lldb"
R2PM_DESC "LLDB backend for radare2"

R2PM_INSTALL() {
	${MAKE} install PYPATH="${R2PM_PYPATH}" PREFIX="${R2PM_PREFIX}" || exit 1
}

R2PM_UNINSTALL() {
	${MAKE} uninstall PYPATH="${R2PM_PYPATH}" PREFIX="${R2PM_PREFIX}" || exit 1
}

R2PM_END
